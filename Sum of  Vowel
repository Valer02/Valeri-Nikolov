import java.sql.SQLOutput;
import java.util.Scanner;

public class zad5 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
            
        //1 четем вход
        //2 циклим думата 
        // 3 правим цикъл с който вадим цифрите от думата 
        // 4 поверка за глдасни 
        //5 сума на гласните 
        //6 отпечатваме резултат

        String text = scanner.nextLine();
        int sum = 0;

        for (int i = 0; i < text.length(); i++) {
            char symbol = text.charAt(i);
            switch (sum){
                case 'a':
                    sum = sum + 1;
                    break;
                case 'e':
                    sum = sum + 2;
                    break;
                case 'i':
                    sum = sum +3;
                    break;
                case 'o':
                    sum = sum +4;
                    break;
                case 'u':
                    sum = sum +5;
                    break;
            }


        }
        System.out.print(sum);
    }

}
